generator client {
    provider = "prisma-client"
    output   = "../../generated"
}

generator clientEdge {
    provider = "prisma-client"
    output   = "../../generated-edge"
    runtime  = "vercel-edge"
}

generator json {
    provider = "prisma-json-types-generator"
}

datasource db {
    provider = "postgresql"
}
